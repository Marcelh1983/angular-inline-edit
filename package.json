{
    "$schema": "./node_modules/ng-packagr/package.schema.json",
    "name": "ngx-edit-inline",
    "version": "0.2.0",
    "description": "Super simple inline edit textbox.",
    "main": "index.js",
    "ngPackage": {
        "lib": {
            "entryFile": "src/index.ts"
        }
    },
    "scripts": {
        "build": "ng-packagr -p package.json",
        "publish": "ng-packagr -p package.json && npm publish ./dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Marcelh1983/angular-inline-edit.git"
    },
    "keywords": [
        "Angular",
        "Word"
    ],
    "author": "Marcel Hoekstra",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Marcelh1983/angular-inline-edit/issues"
    },
    "homepage": "https://github.com/Marcelh1983/angular-inline-edit",
    "devDependencies": {
        "@angular/common": "^8.0.0",
        "@angular/compiler": "^8.0.0",
        "@angular/forms": "^8.0.0",
        "@angular/compiler-cli": "^8.0.0",
        "@angular/core": "^8.0.0",
        "@angular/platform-browser": "^8.0.0",
        "@angular/platform-browser-dynamic": "^8.0.0",
        "@angular/router": "^8.0.0",
        "codelyzer": "~5.2.0",
        "core-js": "^3.4.7",
        "ng-packagr": "^5.7.1",
        "rxjs": "^6.5.3",
        "tslint": "^5.20.1",
        "typescript": "3.5.3",
        "zone.js": "^0.10.2"
    },
    "peerDependencies": {
        "@angular/common": "^8.0.0",
        "@angular/core": "^8.0.0",
        "@angular/forms": "^8.0.0",
        "@angular/platform-browser": "^8.0.0",
        "@angular/router": "^8.0.0",
        "zone.js": "^0.10.2"
    }
}
