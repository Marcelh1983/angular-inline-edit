{
    "$schema": "./node_modules/ng-packagr/package.schema.json",
    "name": "ngx-edit-inline",
    "version": "0.1.2",
    "description": "Super simple inline edit textbox.",
    "main": "index.js",
    "ngPackage": {
        "lib": {
          "entryFile": "src/index.ts"
        }
    },
    "scripts": {
        "build": "ng-packagr -p package.json",
        "publish": "ng-packagr -p package.json && npm publish ./dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Marcelh1983/angular-inline-edit.git"
    },
    "keywords": [
        "Angular",
        "Word"
    ],
    "author": "Marcel Hoekstra",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Marcelh1983/angular-inline-edit/issues"
    },
    "homepage": "https://github.com/Marcelh1983/angular-inline-edit",
    "devDependencies": {
        "@angular/common": "^7.0.0",
        "@angular/compiler": "^7.0.0",
        "@angular/compiler-cli": "^7.0.0",
        "@angular/core": "^7.0.0",
        "@angular/platform-browser": "^7.0.0",
        "@angular/platform-browser-dynamic": "^7.0.0",
        "@angular/router": "^7.0.0",
        "codelyzer": "~5.0.1",
        "core-js": "^2.6.5",
        "ng-packagr": "^5.1.0",
        "rxjs": "^6.3.0",
        "tslint": "^5.16.0",
        "typescript": "3.1.6",
        "zone.js": "^0.8.26"
    },
    "peerDependencies": {
        "@angular/common": "^7.0.0",
        "@angular/core": "^7.0.0",
        "@angular/forms": "^7.0.0",
        "@angular/platform-browser": "^7.0.0",
        "@angular/router": "^7.0.0",
        "zone.js": "^0.8.29"
    }
}
