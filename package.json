{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "ngx-edit-inline",
  "version": "1.1.1",
  "description": "Super simple inline edit textbox.",
  "entryFile": "modules/index.ts",
  "ngPackage": {
    "lib": {
      "entryFile": "modules/index.ts"
    }
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "publish": "ng-packagr -p package.json && npm publish ./dist",
    "start": "ng serve --port 4222"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marcelh1983/angular-inline-edit.git"
  },
  "keywords": [
    "Angular",
    "Inline edit",
    "Inplace edit"
  ],
  "author": "Marcel Hoekstra; Patrick de Klein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marcelh1983/angular-inline-edit/issues"
  },
  "homepage": "https://github.com/Marcelh1983/angular-inline-edit",
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular/cli": "~12.2.17",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/compiler-cli": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "bootstrap": "4.5.2",
    "codelyzer": "^6.0.0",
    "core-js": "^3.6.5",
    "ng-packagr": "^12.2.7",
    "rxjs": "^6.6.3",
    "tslint": "~6.1.3",
    "typescript": "4.3.5",
    "zone.js": "^0.11.8"
  },
  "peerDependencies": {}
}